#!/usr/bin/env bash

set -e

if [[ $# -ne 1 ]]; then
    echo "Error: needs a single working project, which is not provided"
    exit 1
fi

case $1 in

    bunte)
        tmux-sessionizer ${HOME}/devel/bitbucket.org/burdastudios/
        code ${HOME}/.vs-code-workspaces/bunte.code-workspace
        ;;
    *)
        echo "Error: unknown project '$1', exit"
        exit 1
        ;;

esac